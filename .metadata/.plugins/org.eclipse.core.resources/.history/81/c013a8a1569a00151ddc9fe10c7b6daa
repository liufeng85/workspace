ccc=Config()
conf=ccc.get_DB_conf()
aaa= MSSQL(conf)
bbb=aaa.ExecQuery("select * from WC_WarehouseInfo where Wname = \"树木岭网仓\"")
a=bbb[0][1]

def str2bin(strText):
    b = bytes((ord(i) for i in strText))
    return b

def getCode(strText,codec):
    b = bytes((ord(i) for i in strText))  
    return b.decode(codec,'ignore')

c=getCode(a,'gbk')
d="树木岭网仓"
if(c==d):
    print("yes")
else:
    print("oh no!")