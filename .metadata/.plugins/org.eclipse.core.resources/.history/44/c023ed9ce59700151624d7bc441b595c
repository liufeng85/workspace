import os

class Config:
    casepath = ""
    def __init__(self):
        path = os.getcwd()
        f = open(path + "config.txt")
        try:
            for line in f:
                conf_name, value = line.split("=")
                if (conf_name == "casepath"):
                    casepath = value
                elif(conf_name == ""):
                    exportpath = value
                else:
                    continue
        except Exception as e:
            print(e)
        
    def get_case_path(self):
        return casepath
        